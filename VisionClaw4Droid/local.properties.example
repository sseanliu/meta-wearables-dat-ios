# Copy this file to local.properties and fill in your values.
# Do NOT commit local.properties to version control.

# Gemini API key (required) — get one at https://aistudio.google.com/apikey
GEMINI_API_KEY=YOUR_GEMINI_API_KEY

# OpenClaw gateway config (optional — for agentic tool-calling)
OPENCLAW_HOST=http://YOUR_HOST.local
OPENCLAW_PORT=18789
OPENCLAW_HOOK_TOKEN=YOUR_OPENCLAW_HOOK_TOKEN
OPENCLAW_GATEWAY_TOKEN=YOUR_OPENCLAW_GATEWAY_TOKEN

# Meta Wearables DAT SDK — GitHub Packages auth
gpr.user=YOUR_GITHUB_USERNAME
gpr.token=YOUR_GITHUB_TOKEN

# Meta DAT App ID
META_DAT_APP_ID=YOUR_META_APP_ID
